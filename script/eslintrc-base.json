{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      0,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": 0,
    "linebreak-style": 0,
    "no-console": 0,
    "no-undef": 2,
    "no-unused-vars": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "DevToolsAPI": false,
    "InspectorFrontendHost": false,
    "WebInspector": false,
    "WebView": false
  }
}
